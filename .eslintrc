{
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            "error",
            4
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "class-methods-use-this": [
            0
        ],
        "eol-last":[
            0
        ]
    }
}